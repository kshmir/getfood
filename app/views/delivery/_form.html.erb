<%= semantic_form_for @delivery, url: delivery_index_path do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :phone %>
    <%= f.input :delivery_type, as: :select, collection: ["Rotiseria", "Chino", "Sushi", "Pizza", "Empanadas"], include_blank: false %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>

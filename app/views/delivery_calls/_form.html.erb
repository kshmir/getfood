<%= semantic_form_for @delivery_call do |f| %>
  <%= f.inputs do %>
    <%= f.input :delivery_time, as: :time_select %>
    <%= f.input :status %>
    <%= f.input :delivery %>
    <%= f.input :calling_user %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>

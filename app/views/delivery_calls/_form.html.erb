<%= semantic_form_for @delivery_call do |f| %>
  <%= f.inputs do %>
    <%= f.input :delivery_time, as: :select, collection:@times  %>
<!--     <%= f.input :status %> -->
    <%= f.input :delivery, as: :select, collection:@menus %>
    <!-- <%= f.input :calling_user %> -->
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>

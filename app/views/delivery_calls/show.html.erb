<%- model_class = DeliveryCall -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:delivery_time) %>:</strong></dt>
  <dd><%= @delivery_call.delivery_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:delivery_id) %>:</strong></dt>
  <dd><%= @delivery_call.delivery_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:calling_user_id) %>:</strong></dt>
  <dd><%= @delivery_call.calling_user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @delivery_call.status %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              delivery_calls_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_delivery_call_path(@delivery_call), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              delivery_call_path(@delivery_call),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

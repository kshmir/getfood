<%= semantic_form_for @menu, url: delivery_menus_path do |f| %>
  <%= f.inputs do %>
    <%= f.input :description %>
    <%= f.input :price%>  
   <%= f.input :delivery_id,as: :hidden, 
    value: params[:delivery_id] %> 
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
